{
  "manifest_version": 2,
  "name": "FunPay Dark",
  "version": "1.0",
  "description": "Расширение для изменения цветов на funpay.com",
          "icons": {
          "16": "images/icon16.png",
          "32": "images/icon32.png",
          "48": "images/icon48.png",
          "128": "images/icon128.png"
        },
        
  "permissions": [
    "https://funpay.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://funpay.com/*"],
      "js": ["content.js"],
	  "css": ["styles.css"]
    }
  ]
}
